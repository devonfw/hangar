steps:
- name: ${_DOCKER_IMAGE_STEPS}
  args: ['sh', '.pipelines/scripts/build.sh']

- name: ${_DOCKER_IMAGE_STEPS}
  args: ['tar', '-cf', 'build_${BUILD_ID}.tar', '${_TARGET_DIRECTORY}']

# mark to insert step for additonal artifact #

artifacts:
  objects:
    location: gs://${PROJECT_ID}_cloudbuild/pipelinesArtifacts/${pipelineName}/end_time_$(date +%Y-%m-%d_%Hh%M)_commit_${SHORT_SHA}
    paths:
      - build_${BUILD_ID}.tar
